
{# <h2 class="note-header">Image Header</h2> #}

<picture>
	<source srcset="{{TimberImage(post.image).src}}" media="(min-width: 56em)">
	<source srcset="{{TimberImage(post.image).src('large')}}" media="(min-width: 40em)">
	<img class="page-header-image" srcset="{{TimberImage(post.image).src('large')}}" alt="{{post.title}}">
</picture>